<doxygenlayout version="1.0">
    <navindex>
        <tab type="mainpage" visible="yes" title="Home"/>
        <tab type="pages" visible="yes" title="Pages"/>
        <tab type="modules" visible="yes" title="Modules"/>
        <tab type="namespaces" visible="yes" title="Namespaces">
            <tab type="namespacelist" visible="yes" title="List"/>
            <tab type="namespacemembers" visible="yes" title="Members"/>
        </tab>
        <tab type="classes" visible="yes" title="Classes">
            <tab type="classlist" visible="yes" title="List"/>
            <tab type="classindex" visible="$ALPHABETICAL_INDEX" title="Index"/>
            <tab type="hierarchy" visible="yes" title="Hierarchy"/>
            <tab type="classmembers" visible="yes" title="Members"/>
        </tab>
        <tab type="files" visible="yes" title="Files">
            <tab type="filelist" visible="yes" title="List"/>
            <tab type="globals" visible="yes" title="Globals"/>
        </tab>
        <tab type="examples" visible="yes" title="Examples"/>
    </navindex>

    <class>
        <briefdescription visible="yes"/>
        <includes visible="$SHOW_INCLUDE_FILES"/>
        <inheritancegraph visible="$CLASS_GRAPH"/>
        <collaborationgraph visible="$COLLABORATION_GRAPH"/>
        <memberdecl>
            <membergroups visible="yes"/>
        </memberdecl>
        <detaileddescription title=""/>
        <authorsection visible="yes"/>
    </class>

    <namespace>
        <briefdescription visible="yes"/>
        <memberdecl>
            <membergroups visible="yes"/>
        </memberdecl>
        <detaileddescription title=""/>
        <authorsection visible="yes"/>
    </namespace>

    <file>
        <briefdescription visible="yes"/>
        <includes visible="$SHOW_INCLUDE_FILES"/>
        <includegraph visible="$INCLUDE_GRAPH"/>
        <includedbygraph visible="$INCLUDED_BY_GRAPH"/>
        <sourcelink visible="yes"/>
        <detaileddescription title=""/>
    </file>

    <group>
        <briefdescription visible="no"/>
        <detaileddescription title="Description" visible="no"/>
        <groupgraph visible="$GROUP_GRAPHS"/>
        <memberdecl>
            <membergroups visible="yes"/>
        </memberdecl>
        <authorsection visible="yes"/>
    </group>

    <directory>
        <briefdescription visible="yes"/>
        <directorygraph visible="yes"/>
        <memberdecl>
            <dirs visible="yes"/>
            <files visible="yes"/>
        </memberdecl>
        <detaileddescription title=""/>
    </directory>
</doxygenlayout>